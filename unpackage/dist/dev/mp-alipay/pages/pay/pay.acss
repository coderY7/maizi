page {
  position: relativee;
  background-color: #f3f2f4;
}
.content {
  margin: 20rpx 50rpx 0 50rpx;
}
.content .unit1 {
  height: 160rpx;
  border-radius: 30rpx;
  background-color: #ffffff;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.content .unit1 .unit1_top {
  display: flex;
  padding: 30rpx 20rpx 15rpx 20rpx;
  justify-content: space-between;
  border-width: 0 0 0.8px 0;
  border-style: dashed;
  border-color: #D8D8D8;
}
.content .unit1 .unit1_top .unit1_top_left {
  display: flex;
  flex-direction: row;
}
.content .unit1 .unit1_top .unit1_top_left .unit1_top_left2 {
  margin-left: 30rpx;
}
.content .unit1 .unit1_bottom {
  display: flex;
  padding: 15rpx 20rpx 30rpx 20rpx;
  justify-content: space-between;
}
.content .unit2 {
  margin-top: 50rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.content .unit2 .unit2_top {
  border-radius: 20rpx;
  background-color: #ffffff;
  display: flex;
  padding: 10rpx 20rpx 5rpx 20rpx;
  justify-content: space-between;
  border-width: 0 0 0.8px 0;
  border-style: dashed;
  border-color: #D8D8D8;
}
.content .unit2 .unit2_box {
  border-radius: 20rpx;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  border-bottom: 1px solid #c8c7cc;
}
.content .unit2 .unit2_box .unit2_list {
  display: flex;
  flex-direction: row;
  padding: 20rpx;
}
.content .unit2 .unit2_box .unit2_list image {
  width: 110rpx;
  height: 110rpx;
}
.content .unit2 .unit2_box .unit2_list .unit2box_text {
  padding: 20rpx;
  flex: 1;
  display: flex;
  justify-content: space-between;
}
.content .unit2 .unit2_box .unit2_list .unit2box_text .unit2box_text1 {
  flex: 0.8;
  display: flex;
  flex-direction: column;
}
.content .unit2 .unit2_coupon {
  display: flex;
  padding: 10rpx 30rpx 5rpx 20rpx;
  justify-content: space-between;
  border-width: 0 0 0.8px 0;
  border-style: dashed;
  border-color: #D8D8D8;
}
.content .unit2 .unit2_coupon .unit2_counpon_left {
  display: flex;
  justify-content: center;
}
.content .unit2 .unit2_price {
  height: 60rpx;
  display: flex;
  flex-direction: row-reverse;
  margin-top: 10rpx;
  margin-right: 30rpx;
}
.content .unit3 {
  height: 100rpx;
  margin-top: 50rpx;
  border-radius: 30rpx;
  background-color: #ffffff;
  display: flex;
  padding: 10rpx 20rpx 5rpx 20rpx;
  justify-content: space-between;
  align-items: center;
}
.content .unit3 .unit3_left {
  display: flex;
  justify-content: center;
}
.content .unit4 {
  margin-top: 80rpx;
  border-radius: 30rpx;
  background-color: #ffffff;
  width: 100%;
  display: flex;
  flex-direction: column;
  margin-bottom: 200rpx;
}
.content .unit4 .unit4_top {
  display: flex;
  padding: 30rpx 20rpx 15rpx 20rpx;
  justify-content: space-between;
}
.content .unit4 .unit4_bottom {
  display: flex;
  padding: 15rpx 20rpx 30rpx 20rpx;
  justify-content: space-between;
}
.content .btn {
  display: flex;
  flex-direction: row-reverse;
  position: fixed;
  bottom: 20rpx;
  right: 50rpx;
}
.content .btn .btn1 {
  margin-left: 30rpx;
}

