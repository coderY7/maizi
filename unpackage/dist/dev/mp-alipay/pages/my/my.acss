page {
  background: url(/static/main/dt.png) no-repeat;
  background-size: contain;
  background-color: #f3f2f4;
}
.content {
  margin: 100rpx 40rpx 0 40rpx;
}
.content .unit1 {
  display: flex;
  justify-content: content;
  height: 200rpx;
  width: 100%;
  align-items: center;
}
.content .unit1 .image {
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}
.content .unit1 .unit1box {
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.content .unit1 .unit1box .unit1right {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .unit2 {
  margin-top: 50rpx;
  height: 350rpx;
  border: 2rpx solid #ccc;
  border-radius: 20rpx;
  box-shadow: 2rpx 5rpx 5rpx #ccc;
  background-color: #fff;
  width: 100%;
}
.content .unit2 .unit2_top {
  background-color: #e7d2a9;
  border-radius: 20rpx;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .unit2 .unit2_top .unit2_top_left {
  display: flex;
  align-items: center;
}
.content .unit2 .unit2_top .unit2_top_left image {
  margin: 5rpx 10rpx 5rpx 5rpx;
}
.content .unit2 .unit2_bottom {
  display: flex;
  margin-top: 20rpx;
}
.content .unit2 .unit2_bottom .unit2_bottom_item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.content .unit3 {
  height: 200rpx;
  display: flex;
  margin-top: 100rpx;
  border: 2rpx  #ccc;
  border-radius: 20rpx;
  box-shadow: 2rpx 5rpx 5rpx #ccc;
  background-color: #fff;
}
.content .unit3 .unit3Nav {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.content .unit3 .unit3Nav image {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 20rpx;
}
.content .unit4 {
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  bottom: 10rpx;
}

